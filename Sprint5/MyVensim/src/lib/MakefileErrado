all:
	LD_LIBRARY_PATH=$$LD_LIBRARY_PATH:$$PWD
	echo $$LD_LIBRARY_PATH
	g++ -c -fPIC fluxoImpl.cpp -o fluxoImpl.o  
	gcc -shared -o libfluxoImpl.so fluxoImpl.o
	g++ -c -fPIC sistemaImpl.cpp -o sistemaImpl.o  
	gcc -shared -o libsistemaImpl.so sistemaImpl.o
	g++ -c -fPIC modeloImpl.cpp -o modeloImpl.o  
	gcc -shared -o libmodeloImpl.so modeloImpl.o
clean:
	rm *.o

run:
	
	
